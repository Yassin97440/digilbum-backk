spring:
  datasource:
    url: jdbc:mysql://localhost:3306/YOUR_DATABASE
    username: your_username
    password: your_password
    driver-class-name: com.mysql.cj.jdbc.Driver
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: false
  servlet:
    multipart:
      max-file-size: 10MB
      max-request-size: 10MB

pictures:
  server:
    url: https://your.pictures.server.url
  folder:
    path: /pictures/ 